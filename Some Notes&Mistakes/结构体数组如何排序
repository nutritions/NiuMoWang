#include <iostream>
using namespace std;
#include <algorithm>
typedef struct Test
{
       int a;
       int b;
}t;
t test[100];

bool Cmpare( t a,  t b)            //示例为升序
{
     return a.a < b.a;
}

int main()
{
    sort(test, test+ 100,Cmpare);                      
    return 0;
}
